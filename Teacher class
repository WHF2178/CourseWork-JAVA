class Teacher
{
    int TeacherId;
    String TeacherName;
    String Address;
    String WorkingType;
    String EmploymentStatus;
    int WorkingHours;
   Teacher (int TeacherId,String TeacherName,String Address,String WorkingType,String EmploymentStatus,int WorkingHours)
   {
       this.TeacherId=TeacherId;
       this.TeacherName=TeacherName;
       this.Address=Address;
       this.WorkingType=WorkingType;
       this.EmploymentStatus=EmploymentStatus;
       this.WorkingHours=WorkingHours;
   }
   public int getTeacherId()
   {
       return TeacherId;
   }
   public String getTeacherName()
   {
       return TeacherName;
   }
   public String getAddress()
   {
       return Address;
   }
   public String getWorkingType()
   {
       return WorkingType;
   }
   public String getEmploymentStatus()
   {
       return EmploymentStatus;
   }
   public int getWorkingHours()
   {
       return WorkingHours;
   }
   public void setWorkingHours(int newWorkingHours)
   {
      this.WorkingHours=newWorkingHours;
   }
   
   public void display()
   {
     System.out.println("Teacher Id is  " + TeacherId);
     System.out.println("Teacher Name is " + TeacherName);
     System.out.println("Address is " + Address);
     System.out.println("Working Type is " + WorkingType);
     System.out.println("Employment Status is " +EmploymentStatus);
     if(WorkingHours>0)
     {
      System.out.println("Working Hours is " + WorkingHours);   
     }
     else
     {
         System.out.println("Working Hours isnot assigned");
     }
   }
   
}
